<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sbzze.travelfriend.mapper.TeamAlbumImagesMapper" >
  <resultMap id="BaseResultMap" type="com.sbzze.travelfriend.entity.TeamAlbumImages" >
    <constructor >
      <idArg column="id" jdbcType="CHAR" javaType="java.lang.String" />
      <arg column="album_id" jdbcType="CHAR" javaType="java.lang.String" />
      <arg column="image_type" jdbcType="VARCHAR" javaType="java.lang.String" />
      <arg column="address" jdbcType="VARCHAR" javaType="java.lang.String" />
      <arg column="compress_address" jdbcType="VARCHAR" javaType="java.lang.String" />
    </constructor>
  </resultMap>
  <select id="findImagesByAlbumId" resultType="com.sbzze.travelfriend.entity.TeamAlbumImages">
      SELECT * FROM team_album_images
      where
      album_id=#{albumId}
   </select>
   <select id="findImagesByAlbumIdAndImageType" resultType="com.sbzze.travelfriend.entity.TeamAlbumImages">
      SELECT * FROM team_album_images
      where
      album_id=#{albumId} and image_type=#{imageType}
   </select>



</mapper>